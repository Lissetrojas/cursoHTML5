            function update_counter(total){

                        var url = "/save_counter/"+471625+"/"+total;
                        $.get(url,function(data){});
                    } 
                      (function(){
    
                        if (document.getElementById('facebook-jssdk')) {return;}

                        var firstScriptElement = document.getElementsByTagName('script')[0];

                        var facebookJS = document.createElement('script'); 
                        facebookJS.id = 'facebook-jssdk';

                        facebookJS.src = '//connect.facebook.net/en_US/all.js';

                        firstScriptElement.parentNode.insertBefore(facebookJS, firstScriptElement);
                       }());
                       
        window.fbAsyncInit = function() {

            FB.init({
                appId  : '266047570124470',
                status: true, // check login status
                cookie: true, // enable cookies to allow server to access session,
                xfbml: true, // enable XFBML and social plugins
                oauth: true
            });

            
                var query = 'select total_count from link_stat where url = "http://www.elespectador.com/noticias/politica/fueron-elegidos-los-jurados-de-votacion-los-comicios-de-articulo-471625"';
                var commentQuery = FB.Data.query(query);

                var commentRow;
                FB.Data.waitOn([commentQuery], function() {
               
                    commentRow = commentQuery.value[0];
                    commentRow = commentRow.total_count
                    $(".facebook_node_471625 .count-share-content-facebook").html(commentRow);
                    var nvcont =$('#ee-nuevo-despliegue-redes-sociales .total_node_471625');
                    $('span',nvcont).remove();
                    var comentarios = parseInt(nvcont.text());
                    var total = comentarios+commentRow;

                    $.getJSON("http://urls.api.twitter.com/1/urls/count.json?url=http://www.elespectador.com/noticias/politica/fueron-elegidos-los-jurados-de-votacion-los-comicios-de-articulo-471625&callback=?",
                        function(data) {
                            $(".twitter_node_471625 .count-share-content-twitter").html(data.count);
                            var nvcont =$('#ee-nuevo-despliegue-redes-sociales .total_node_471625');
                            $('span',nvcont).remove();
                            total = total+data.count;
                            nvcont.html(total+'<span>Compartido</span>');
                            update_counter(total-comentarios);
                    });
                });
            };                                
